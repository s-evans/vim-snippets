snippet bd cuda
	blockDim.${1:x}
snippet bi cuda
	blockIdx.${1:x}
snippet ti cuda
	threadIdx.${1:x}
snippet gd cuda
	gridDim.${1:x}
snippet _g cuda
	__global__
snippet _d cuda
	__device__
snippet _s cuda
