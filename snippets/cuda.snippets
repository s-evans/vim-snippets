# Functions
#
snippet devsync cuda
	cudaDeviceSynchronize();
snippet memcpy cuda
	cudaMemcpy(${1:d}, ${2:s}, ${3:size}, ${4:cudaMemcpyDeviceToHost})
snippet d2h cuda
	cudaMemcpyDeviceToHost
snippet h2d cuda
	cudaMemcpyHostToDevice
snippet mallocd cuda
	cudaMalloc(&${1:dp}, ${2:size})
snippet freed cuda
	cudaFree(${1:dp})
snippet freeh cuda
	cudaFreeHost(${1:hp})
snippet alloch cuda
	cudaHostAlloc(${1:size})
snippet malloch cuda
	cudaMallocHost(${1:size})
snippet mallocr cuda
	cudaHostRegister(${1:hp})
snippet memcpya cuda
	cudaMemcpyAsync(${1:d}, ${2:s}, ${3:size}, ${4:cudaMemcpyDeviceToHost}, ${5:stream})
snippet memset cuda
	cudaMemset(${1:dp}, ${2:v}, ${3:size})
snippet memseta cuda
	cudaMemsetAsync(${1:dp}, ${2:v}, ${3:size}, ${4:stream})
snippet streamc cuda
	cudaStreamCreate()
snippet streamd cuda
	cudaStreamDestroy()
